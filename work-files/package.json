{
  "name": "ccm-webcomponent",
  "version": "0.0.0",
  "engines": {
    "node": "^16.11.0",
    "npm": "^8.0.0"
  },
  "scripts": {
    "nx": "ng",
    "start": "nx serve ccm-webcomponent --proxy-config proxy.config.js --host local.dev.finra.org --ssl true",
    "start:proxy": "nx serve ccm-webcomponent ---proxy-config proxy.config.js --host local.dev.finra.org --ssl false",
    "mock:server": "npx json-server --port 4300 --watch mocks/data.json",
    "dev": "concurrently --kill-others \"npm run mock:server\" \"npm run start:proxy\"",
    "build:debug": "nx build ccm-webcomponent --output-hashing=none --optimization=false && node build.web.components.js",
    "watch": "nx build ccm-webcomponent ---watch --configuration development",
    "test": "nx test ccm-webcomponent",
    "test:headless": "nx test ccm-webcomponent --browsers ChromeHeadless",
    "build:prod": "node generate-build-info.js && nx build ccm-webcomponent && node build.web.components.js && node run.sonar.js",
    "build:prod:nosonar": "node generate-build-info.js && nx build --output-hashing=none && ng buil d --optimization=false --output-hashing=none --output-path=dist/ccm-webcomponent/debug && node build.web.components.js",
    "start:prod": "npx http-server dist/ccm-webcomponent/ --port 8088",
    "sonar": "node run.sonar.js",
    "start:client1": "npx nx run client-context-webcomponent-ri:serve --configuration=development"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "apps/ccm-webcomponent/src/**/*": [
      "eslint --fix"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
