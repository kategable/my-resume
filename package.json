{
  "name": "ccm-webcomponent",
  "version": "0.0.0",
  "engines": {
    "node": "^16.11.0",
    "npm": "^8.0.0"
  },
  "scripts": {
    "nx": "ng",
    "start": "nx serve ccm-webcomponent --proxy-config proxy.config.js --host local.dev.finra.org --ssl true",
    "start:proxy": "nx serve ccm-webcomponent ---proxy-config proxy.config.js --host local.dev.finra.org --ssl false",
    "mock:server": "npx json-server --port 4300 --watch mocks/data.json",
    "dev": "concurrently --kill-others \"npm run mock:server\" \"npm run start:proxy\"",
    "build:debug": "nx build ccm-webcomponent --output-hashing=none --optimization=false && node build.web.components.js",
    "watch": "nx build ccm-webcomponent ---watch --configuration development",
    "test": "nx test ccm-webcomponent",
    "test:headless": "nx run ccm-webcomponent:test --no-watch",
    "build:prod": "node generate-build-info.js && nx run ccm-webcomponent:build:production --verbose && node lscache.js && npm run test:headless && node run.sonar.js &&  node build.web.components.js",
    "build:prod:nosonar": "node generate-build-info.js  && nx build --output-hashing=none && ng build --optimization=false --output-hashing=none --output-path=dist/ccm-webcomponent/debug && node build.web.components.js",
    "start:prod": "npx http-server dist/ccm-webcomponent/ --port 8088",
    "sonar": "node run.sonar.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "apps/ccm-webcomponent/src/**/*": [
      "eslint --fix"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.4",
    "@angular/common": "16.2.4",
    "@angular/compiler": "16.2.4",
    "@angular/core": "16.2.4",
    "@angular/elements": "16.2.4",
    "@angular/forms": "16.2.4",
    "@angular/platform-browser": "16.2.4",
    "@angular/platform-browser-dynamic": "16.2.4",
    "@finra/ctl-common": "2.2.0",
    "@finra/grid": "1.5.8",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "ag-grid-angular": "29.0.0",
    "ag-grid-community": "29.0.0",
    "ag-grid-enterprise": "29.0.0",
    "aws-sdk": "^2.1343.0",
    "eslint-config-prettier": "^8.8.0",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "moment": "^2.29.4",
    "quill": "^1.3.7",
    "rxjs": "~7.5.0",
